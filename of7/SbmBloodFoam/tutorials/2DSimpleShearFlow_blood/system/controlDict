application     SbmBloodFoam;

startFrom       startTime;

startTime       0.0;

stopAt          endTime;

endTime         10;

deltaT          0.000001;

writeControl    timeStep;

writeInterval   500000;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

